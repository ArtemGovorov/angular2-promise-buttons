{
  "compilerOptions": {
    "outDir": "../dist",
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": false,
    "noImplicitAny": true,
    "declaration": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "lib": [
      "dom",
      "es6"
    ],
    "types": [
    ],
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "../scripts/typings.d.ts",
    "./index.ts"
  ],
  "angularCompilerOptions": {
    "genDir": "../tmp/factories"
  }
}
